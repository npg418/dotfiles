{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "console_title_template": "{{ if .Root }}Û±êã {{ end }}{{ .Shell }} in {{ if .Segments.Session.SSHSession }}{{ .UserName }}@{{ .HostName }}:{{ end }}{{ .PWD }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "p:black",
          "template": "‚ï≠‚îÄ"
        },
        {
          "type": "os",
          "style": "diamond",
          "leading_diamond": "ÓÇ∂",
          "trailing_diamond": "",
          "foreground": "p:yellow",
          "background": "p:black",
          "template": " {{ if .WSL }}Ôåö on {{ end }}{{.Icon}} "
        },
        {
          "type": "session",
          "style": "powerline",
          "powerline_symbol": "ÓÇº",
          "foreground": "p:black",
          "background": "p:yellow",
          "template": " {{ if .SSHSession }}ÓÆ© {{ end }}{{ .UserName }}{{ if .SSHSession }}@{{ .HostName }}{{ end }} "
        },
        {
          "type": "path",
          "style": "{{ if .Segments.Git.Dir }}powerline{{ else }}diamond{{ end }}",
          "powerline_symbol": "ÓÇº",
          "leading_diamond": "",
          "trailing_diamond": "ÓÇ¥",
          "foreground": "p:text",
          "background": "p:blue",
          "template": " {{ if .Segments.Git.Dir }}Ó©¢ {{ url .Segments.Git.RepoName .Segments.Git.UpstreamURL }}{{ .PWD | replace .Segments.Git.Dir \"\" }}{{ else }}{{ .Path }}{{ end }} ",
          "properties": {
            "style": "mixed"
          }
        },
        {
          "type": "git",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "ÓÇ¥",
          "foreground": "p:text",
          "background": "p:green",
          "template": " {{ .UpstreamIcon }} {{ .HEAD }}{{ if (gt .Ahead 0) }} <p:red>Ôê±{{ .Ahead }}</>{{ end }}{{ if (gt .Behind 0) }} <p:magenta>Ôê≥{{ .Behind }}</>{{ end }}{{ if .Working.Changed }} <p:yellow>{{ .Working.String }}</>{{ end }}{{ if .Staging.Changed }} <p:blue>{{ .Staging.String }}</>{{ end }} ",
          "properties": {
            "fetch_upstream_icon": true,
            "fetch_status": true,
            "branch_icon": "Óú• "
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "cmake",
          "style": "diamond",
          "leading_diamond": "<#F32735>ÓÇ∂</>",
          "trailing_diamond": "<#0068C7,transparent>ÓÇ¥</>",
          "foreground": "p:text",
          "background": "transparent",
          "template": "<,#F32735> Óòû Óòù </><#F32735,#3EAE2B>ÓÇº</><,#3EAE2B> cmake </><#3EAE2B,#0068C7>ÓÇº</><,#0068C7> {{ .Full }} </>"
        },
        {
          "type": "deno",
          "style": "diamond",
          "leading_diamond": "ÓÇ∂",
          "trailing_diamond": "ÓÇ¥",
          "foreground": "p:text",
          "background": "#FFFFFF",
          "template": " ü¶ï {{ .Full }} "
        },
        {
          "type": "docker",
          "style": "diamond",
          "leading_diamond": "ÓÇ∂",
          "trailing_diamond": "ÓÇ¥",
          "foreground": "p:text",
          "background": "#1D63ED",
          "template": " Ôåà {{ .Context }} "
        },
        {
          "type": "time",
          "style": "diamond",
          "leading_diamond": "ÓÇ∂",
          "trailing_diamond": "ÓÇ¥",
          "foreground": "p:white",
          "background": "p:black"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "p:black",
          "template": "‚ï∞‚îÄ"
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "p:white",
          "template": "‚ùØ "
        }
      ]
    }
  ],
  "palette": {
    "text": "#24283b",
    "black": "#414868",
    "blue": "#7aa2f7",
    "green": "#73daca",
    "magenta": "#bb9af7",
    "red": "#f7768e",
    "white": "#c0caf5",
    "yellow": "#e0af68"
  }
}
